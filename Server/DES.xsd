				<xs:attribute name="name" type="KeyType" use="required">
					<xs:annotation>
						<xs:documentation>Name des Elements, der innerhalb der Struktur eindeutig sein muss.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="icon" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Definiert den Pfad zum Symbol des Knotens.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
  </xs:complexType>

